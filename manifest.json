{
	"manifest_version": 3,
	"name": "Game Solver",
	"description": "solve a puzzle game automatically",
	"version": "0.1",
	"content_scripts": [
		{"matches": [ "https://www.puzzle-minesweeper.com/minesweeper*/"], "js": ["minesweeper.js"]},
		{"matches": [ "https://www.puzzle-minesweeper.com/mosaic-*/"], "js": ["mosaic.js"]},
		{"matches": [ "https://www.puzzle-nonograms.com/*", "https://nonogramsonline.com/*" ], "js": ["nonograms.js"]},
		{"matches": [ "https://www.puzzle-skyscrapers.com/*"], "js": ["skyscrapers.js"]}
	]
}
